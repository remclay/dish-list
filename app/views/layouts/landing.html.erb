<!DOCTYPE html>
<html>
  <head>
    <title>DishList</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag 'landing', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="top">
        <%= render partial: "layouts/header" %>
        <%= render partial: "layouts/profile_links" %>
      </div>

      <div class="content">
        <%= render partial: "layouts/errors" %>
        <%= yield %>

        <div class="index-images">
          <%= render partial: "layouts/placeholder_images" %>
        </div>
      </div>

    </div>
  </body>
</html>
